{
  "name": "bloch-vscode",
  "displayName": "bloch-vscode",
  "description": "The official VSCode extension for the Bloch quantum programming language!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bloch",
      "aliases": ["Bloch", "bloch"],
      "extensions": [".bloch"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bloch",
      "scopeName": "source.bloch",
      "path": "./syntaxes/bloch.tmLanguage.json"
    }]
  }
}
